@model List<Coche>

<h1>Coches Favoritos AWS ElastiCache</h1>

@if (Model != null)
{
	@foreach (Coche car in Model)
	{
		<div class="card" style="width: 18rem">
			<img src="@car.Imagen" class="img-card-top" />
			<div class="card-body">
				<h5 class="card-title">@car.Marca @car.Modelo</h5>
				<a asp-controller="Coches" asp-action="DeleteFavorito" asp-route-idcoche="@car.IdCoche" class="btn btn-primary">
					Eliminar Favorito
				</a>
			</div>
		</div>
		
	}

}
else
{
	<h1>No existen coches favoritos</h1>
}